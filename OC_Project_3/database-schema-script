CREATE CACHED TABLE PUBLIC.PRODUCT(
    ID VARCHAR(255) NOT NULL,
    CATEGORY VARCHAR(255),
    DESCRIPTION VARCHAR(255),
    NAME VARCHAR(255),
    PRICE DOUBLE,
    TYPE VARCHAR(255)
);           
ALTER TABLE PUBLIC.PRODUCT ADD CONSTRAINT PUBLIC.CONSTRAINT_1 PRIMARY KEY(ID);
